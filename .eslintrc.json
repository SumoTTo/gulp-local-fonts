{
	"env": {
		"browser": false,
		"node": true
	},
	"extends": [
		"plugin:@wordpress/eslint-plugin/recommended"
	],
	"overrides": [
		{
			"files": [
				"src/js/*.js"
			],
			"env": {
				"browser": true,
				"node": false
			}
		},
		{
			"files": [
				"test/test.js"
			],
			"env": {
				"browser": false,
				"node": true,
				"jest": true
			}
		}
	]
}
